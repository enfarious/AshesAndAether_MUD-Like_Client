{
  "serverUrl": "http://localhost:3100",
  "sendMode": "event",
  "receiveMode": "event",
  "sendEventName": "message",
  "receiveEventName": "message",
  "protocolVersion": "1.0.0",
  "clientVersion": "0.1.0",
  "maxUpdateRate": 1,
  "showDiagnosticInfo": true,
  "autoConnect": true,
  "defaultCommandType": "chat",
  "theme": "ember",
  "customTheme": {
    "normalForeground": "BrightWhite",
    "normalBackground": "Black",
    "accentForeground": "BrightCyan",
    "accentBackground": "Black",
    "mutedForeground": "DarkGray"
  },
  "positionCommandTemplate": "position {target} {range_band} {angle_deg}",
  "rangeBands": [
    "melee_close",
    "melee_long",
    "ranged_short",
    "ranged_long"
  ],
  "macros": [
    { "label": "North", "command": "Walk.N" },
    { "label": "South", "command": "Walk.S" },
    { "label": "East", "command": "Walk.E" },
    { "label": "West", "command": "Walk.W" },
    { "label": "Stop", "command": "Stop" },
    { "label": "Look", "command": "look" },
    { "label": "Who", "command": "who" },
    { "label": "Inv", "command": "inventory" }
  ]
}
